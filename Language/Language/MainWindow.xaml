<Window x:Class="Language.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Language"
        xmlns:localUC="clr-namespace:Language.UCs"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1050"
        MinWidth="1050">
    <Grid x:Name="mainGrid">
        <localUC:MainMenuUC x:Name="Menu" Visibility="Visible"/>
        <localUC:ClientsUC x:Name="Clients" Visibility="Collapsed"/>
    </Grid>
</Window>
